    <complexType name="DelegationAgreement">
        <attribute name="id" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        An ID uniquely identifying the agreement.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="name" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The name of the agreement.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="description" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A description of the agreement. Must be less than 255 characters.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="allowSubAgreements" type="xsd:boolean" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Deprecated. Use subagreementnestlevel instead. Whether this DA can be used by the delegates as
                        the basis for creating another DA
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="subAgreementNestLevel" type="xsd:nonNegativeInteger" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        How many levels of nested subagreements can be created underneath this one. A value of 0
                        disallows any subagreements. The maximum value is application, and environment,
                        specific.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="parentDelegationAgreementId" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        If this DA is a nested DA, the parent of this DA.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="principalType" type="RAX-AUTH:PrincipalType" use="optional"/>
        <attribute name="principalId" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Id of the principal
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="domainId" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The domain against which the delegation agreement grants access.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

