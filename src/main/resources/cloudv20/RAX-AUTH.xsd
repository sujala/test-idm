    </simpleType>

    <simpleType name="RoleTypeEnum">
        <restriction base="xsd:string">
            <enumeration value="STANDARD">
                <annotation>
                    <xsd:documentation
                            xml:lang="EN"
                            xmlns="http://www.w3.org/1999/xhtml">
