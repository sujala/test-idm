<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xslt/schema.xslt"?>

<!-- (C) 2011 OpenStack LLC., All Rights Reserved -->

<schema
        elementFormDefault="qualified"
        attributeFormDefault="unqualified"
        xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:identity="http://docs.openstack.org/identity/api/v2.0"
        xmlns:RAX-AUTH="http://docs.rackspace.com/identity/api/ext/RAX-AUTH/v1.0"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:OS-KSCATALOG="http://docs.openstack.org/identity/api/ext/OS-KSCATALOG/v1.0"
        xmlns:xsdxt="http://docs.rackspacecloud.com/xsd-ext/v1.0"
        targetNamespace="http://docs.rackspace.com/identity/api/ext/RAX-AUTH/v1.0">

    <import namespace="http://docs.openstack.org/identity/api/v2.0" schemaLocation="token.xsd" />
    <import namespace="http://docs.openstack.org/identity/api/v2.0" schemaLocation="user.xsd" />

    <import namespace="http://docs.openstack.org/identity/api/v2.0" schemaLocation="api.xsd" />
    <import namespace="http://docs.rackspace.com/identity/api/ext/RAX-KSKEY/v1.0" schemaLocation="RAX-KSKEY-credentials.xsd" />
    <import namespace="http://docs.openstack.org/identity/api/ext/OS-KSCATALOG/v1.0" schemaLocation="OS-KSCATALOG.xsd" />

    <attribute name="defaultRegion" type="xsd:string" />
    <attribute name="domainId" type="xsd:string" />
    <attribute name="contactId" type="xsd:string" />

    <attribute name="propagate" type="xsd:boolean" />
    <attribute name="administratorRole" type="xsd:string" />

    <attribute name="federatedIdp" type="xsd:string" />
    <attribute name="tenantAlias" type="xsd:string" />
    <attribute name="multiFactorEnabled" type="xsd:boolean"/>
    <attribute name="multiFactorState" type="RAX-AUTH:MultiFactorStateEnum" />
    <attribute name="userMultiFactorEnforcementLevel" type="RAX-AUTH:UserMultiFactorEnforcementLevelEnum" />
    <attribute name="scope" type="RAX-AUTH:ScopeEnum" />
    <attribute name="federationType" type="RAX-AUTH:IdentityProviderFederationTypeEnum" />
    <attribute name="pemEncoding" type="xsd:string" />
    <attribute name="tokenFormat" type="RAX-AUTH:TokenFormatEnum" />
    <attribute name="factorType" type="RAX-AUTH:FactorTypeEnum" />
    <attribute name="assignmentType" type="RAX-AUTH:EndpointTemplateAssignmentTypeEnum" />
    <attribute name="assignment" type="RAX-AUTH:RoleAssignmentEnum" />
    <attribute name="roleType" type="RAX-AUTH:RoleTypeEnum" />
    <attribute name="issued" type="xsd:dateTime" />
    <attribute name="passwordExpiration" type="xsd:dateTime" />
    <attribute name="delegationAgreementId" type="xsd:string"/>

    <attribute name="sessionInactivityTimeout" type="xsd:duration">
        <annotation>
            <xsd:documentation>
                <p>
                    Session inactivity timeout property used across all Rackspace UIs
                </p>
            </xsd:documentation>
        </annotation>
    </attribute>

    <element name="authenticatedBy" type="RAX-AUTH:AuthenticatedBy" />
    <element name="defaultRegionServices" type="RAX-AUTH:DefaultRegionServices" />
    <element name="impersonation" type="RAX-AUTH:ImpersonationRequest" />
    <element name="access" type="RAX-AUTH:ImpersonationResponse" />
    <element name="impersonator" type="identity:UserForAuthenticateResponse" >
    	<annotation>
            <xsd:documentation>
                <p>The user impersonating.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="domain" type="RAX-AUTH:Domain" >
        <annotation>
            <xsd:documentation>
                <p>A domain represents an administrative boundary for identity management.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="mobilePhone" type="RAX-AUTH:MobilePhone" >
        <annotation>
            <xsd:documentation>
                <p>A mobile phone represents an mobile phone capable of being used for multifactor.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="mobilePhones" type="RAX-AUTH:MobilePhones" >
        <annotation>
            <xsd:documentation>
                <p>A mobilePhones represents a list of mobile phone capable of being used for multifactor.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="verificationCode" type="RAX-AUTH:VerificationCode" >
        <annotation>
            <xsd:documentation>
                <p>A verification code represents a code used for verification of something.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="multiFactor" type="RAX-AUTH:MultiFactor" >
        <annotation>
            <xsd:documentation>
                <p>Settings for multifactor authentication.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="domains" type="RAX-AUTH:Domains" />
    <element name="regions" type="RAX-AUTH:Regions" />
    <element name="region" type="RAX-AUTH:Region" />
    <element name="questions" type="RAX-AUTH:Questions" />
    <element name="question" type="RAX-AUTH:Question" />
    <element name="secretqas" type="RAX-AUTH:SecretQAs" />
    <element name="secretqa" type="RAX-AUTH:SecretQA" />

    <element name="phonePin" type="RAX-AUTH:PhonePin" >
        <annotation>
            <xsd:documentation>
                <p>A numeric phone PIN that is assigned to the user when created.</p>
            </xsd:documentation>
        </annotation>
    </element>

    <element name="rsaCredentials" type="RAX-AUTH:RsaCredentials" substitutionGroup="identity:credential" />
    <element name="passcodeCredentials" type="RAX-AUTH:PasscodeCredentials" substitutionGroup="identity:credential" />
    <element name="forgotPasswordCredentials" type="RAX-AUTH:ForgotPasswordCredentials" />
    <element name="changePasswordCredentials" type="RAX-AUTH:ChangePasswordCredentials" />
    <element name="delegationCredentials" type="RAX-AUTH:DelegationCredentials" substitutionGroup="identity:credential" />
    <element name="bypassCodes" type="RAX-AUTH:BypassCodes"/>
    <element name="multiFactorDomain" type="RAX-AUTH:MultiFactorDomain"/>
    <element name="tenantType" type="RAX-AUTH:TenantType" />
    <element name="tenantTypes" type="RAX-AUTH:TenantTypes" />
    <element name="identityProvider" type="RAX-AUTH:IdentityProvider"/>
    <element name="identityProviders" type="RAX-AUTH:IdentityProviders">
        <annotation>
            <xsd:documentation>
                <p>A list of Identity Providers.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="publicCertificate" type="RAX-AUTH:PublicCertificate"/>
    <element name="publicCertificates" type="RAX-AUTH:PublicCertificates"/>
    <element name="approvedDomainIds" type="RAX-AUTH:ApprovedDomainIds"/>
    <element name="emailDomains" type="RAX-AUTH:EmailDomains"/>
    <element name="otpDevice" type="RAX-AUTH:OTPDevice" >
        <annotation>
            <xsd:documentation>
                <p>A OTP device capable of being used for multifactor.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="otpDevices" type="RAX-AUTH:OTPDevices" >
        <annotation>
            <xsd:documentation>
                <p>A list of OTP devices capable of being used for multifactor.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="multiFactorDevices" type="RAX-AUTH:MultiFactorDevices" >
        <annotation>
            <xsd:documentation>
                <p>A list of multi factor devices.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="tenantTypeEndpointRule" type="RAX-AUTH:TenantTypeEndpointRule"/>
    <element name="endpointAssignmentRules" type="RAX-AUTH:EndpointAssignmentRules" >
        <annotation>
            <xsd:documentation>
                <p>A list of endpoint assignment rules.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="tenantTypeEndpointRules" type="RAX-AUTH:TenantTypeEndpointRules" >
        <annotation>
            <xsd:documentation>
                <p>A list of tenant type endpoint assignment rules.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="types" type="RAX-AUTH:Types" >
        <annotation>
            <xsd:documentation>
                <p>A list of tenant type.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="identityProperty" type="RAX-AUTH:IdentityProperty" >
        <annotation>
            <xsd:documentation>
                <p>An Identity property.</p>
            </xsd:documentation>
        </annotation>
    </element>
    <element name="domainAdministratorChange" type="RAX-AUTH:DomainAdministratorChange" >
        <annotation>
            <xsd:documentation>
                <p>A request to change the administrator of a domain.</p>
            </xsd:documentation>
        </annotation>
    </element>

    <element name="userGroup" type="RAX-AUTH:UserGroup" />
    <element name="userGroups" type="RAX-AUTH:UserGroups" />
    <element name="tenantAssignment" type="RAX-AUTH:TenantAssignment" />
    <element name="tenantAssignments" type="RAX-AUTH:TenantAssignments" />
    <element name="roleAssignments" type="RAX-AUTH:RoleAssignments" />
    <element name="delegationAgreement" type="RAX-AUTH:DelegationAgreement" />
    <element name="delegationAgreements" type="RAX-AUTH:DelegationAgreements" />
    <element name="delegateReference" type="RAX-AUTH:DelegateReference" />
    <element name="delegateReferences" type="RAX-AUTH:DelegateReferences" >
        <annotation>
            <xsd:documentation>
                <p>A list of delegates.</p>
            </xsd:documentation>
        </annotation>
    </element>

    <!-- Complex Types -->
    <complexType name="Types">
        <sequence>
            <element name="type" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="TenantType">
        <attribute name="name" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The name of the tenant type.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="description" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The description of the tenant type.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="TenantTypes">
        <sequence>
            <element name="tenantType" type="RAX-AUTH:TenantType" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="AuthenticatedBy">
        <sequence>
            <element name="credential" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <simpleType name="MultiFactorStateEnum">
        <restriction base="xsd:string">
            <enumeration value="ACTIVE"/>
            <enumeration value="LOCKED"/>
        </restriction>
    </simpleType>

    <simpleType name="ScopeEnum">
        <restriction base="xsd:string">
            <enumeration value="SETUP-MFA"/>
            <enumeration value="MFA-SESSION-ID"/>
        </restriction>
    </simpleType>

    <complexType name="ImpersonationRequest">
        <sequence>
            <element name="user" type="identity:User" >
                <annotation>
                    <xsd:documentation>
                        <p>The user being impersonated.</p>
                    </xsd:documentation>
                </annotation>
            </element>
            <element name="expire-in-seconds" type="int" minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="ImpersonationResponse">
        <sequence>
            <element name="token" type="identity:Token">
                <annotation>
                    <xsd:documentation>
                        <p>A token representing the impersonated user.</p>
                    </xsd:documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="MultiFactorDevice" abstract="true">
        <attribute name="id" type="xsd:string">
        <annotation>
            <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                <p>
                    An ID uniquely identifying the device.
                </p>
            </xsd:documentation>
        </annotation>
        </attribute>
        <anyAttribute namespace="##other" processContents="lax"/>
    </complexType>

    <complexType name="MobilePhone">
        <complexContent>
            <extension base="RAX-AUTH:MultiFactorDevice">
                <attribute name="number" type="xsd:string">
                    <annotation>
                        <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                            <p>
                                The phone number of the mobile phone.
                            </p>
                        </xsd:documentation>
                    </annotation>
                </attribute>
                <attribute name="verified" type="xsd:boolean">
                    <annotation>
                        <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                            <p>
                                Whether the device has been verified for the associated user.
                            </p>
                        </xsd:documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="MobilePhones">
        <sequence>
            <element name="mobilePhone" type="RAX-AUTH:MobilePhone" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
        <anyAttribute namespace="##other" processContents="lax"/>
    </complexType>
    <complexType name="VerificationCode">
        <attribute name="code" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The code used for verification.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>
    <complexType name="MultiFactor">
        <attribute name="enabled" type="xsd:boolean" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Whether to enable or disable multi-factor
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="unlock" type="xsd:boolean" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        After a number of unsuccessful multifactor authentication attempts a user's account will become locked.
                        When set to true, and the caller has appropriate permissions, this will unlock the user's account if
                        the user has multifactor enabled. When set to false or not provided, no change will be made to
                        the lock status of the user's account.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="userMultiFactorEnforcementLevel" type="RAX-AUTH:UserMultiFactorEnforcementLevelEnum" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        An enumeration indicating if user has required multifactor.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="factorType" type="RAX-AUTH:FactorTypeEnum" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Type of the second type of factor that will be required to authenticate when multi-factor is enabled.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>
    <complexType name="Domain">
        <sequence>
            <element name="description" type="xsd:string" minOccurs="0">
                <annotation>
                    <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                        <p>
                            A free text description of the domain.
                        </p>
                    </xsd:documentation>
                </annotation>
            </element>
        </sequence>
        <attribute name="id" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        An ID uniquely identifying the domain.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="name" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The name of the domain.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="enabled" type="xsd:boolean" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A boolean indicating if domain is active or not.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="domainMultiFactorEnforcementLevel" type="RAX-AUTH:DomainMultiFactorEnforcementLevelEnum" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        An enumeration indicating if domain has required multifactor.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="sessionInactivityTimeout" type="xsd:duration" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Session inactivity timeout property used across all Rackspace UIs
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="rackspaceCustomerNumber" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The rackspace customer number.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="Domains">
        <sequence>
            <element name="domain" type="RAX-AUTH:Domain" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="DefaultRegionServices">
        <sequence>
            <element name="serviceName" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                        <p>
                            Service name.
                        </p>
                    </xsd:documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="Region">
        <attribute name="name" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The name of the region.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="isDefault" type="xsd:boolean" use="optional" default="false">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A boolean indicating if region is active or not.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="enabled" type="xsd:boolean" use="optional" default="false">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A boolean indicating if region is active or not.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="Regions">
        <sequence>
            <element name="region" type="RAX-AUTH:Region" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="Question">
        <attribute name="id" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The id of the question.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="question" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A question.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="Questions">
        <sequence>
            <element name="question" type="RAX-AUTH:Question" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="PhonePin">
        <attribute name="pin" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Unique numeric pin.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="SecretQA">
        <attribute name="id" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The id of the question.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="question" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The question.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="answer" type="xsd:string" use="required">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The answer.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="SecretQAs">
        <sequence>
            <element name="secretqa" type="RAX-AUTH:SecretQA" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="RsaCredentials">
        <complexContent>
            <extension base="identity:CredentialType">
                <attribute name="username" type="xsd:string" use="required" />
                <attribute name="tokenKey" type="xsd:string" use="required" />
            </extension>
        </complexContent>
    </complexType>

    <complexType name="PasscodeCredentials">
        <complexContent>
            <extension base="identity:CredentialType">
                <attribute name="passcode" type="xsd:string" use="required" />
            </extension>
        </complexContent>
    </complexType>

    <complexType name="ChangePasswordCredentials">
        <complexContent>
            <extension base="identity:CredentialType">
                <attribute name="username" type="xsd:string" use="required" />
                <attribute name="password" type="xsd:string" use="required" />
                <attribute name="newPassword" type="xsd:string" use="required" />
            </extension>
        </complexContent>
    </complexType>

    <complexType name="ForgotPasswordCredentials">
        <complexContent>
            <extension base="identity:CredentialType">
                <attribute name="username" type="xsd:string" use="optional" />
                <attribute name="portal" type="xsd:string" use="optional" />
            </extension>
        </complexContent>
    </complexType>

    <complexType name="DelegationCredentials">
        <complexContent>
            <extension base="identity:CredentialType">
                <attribute name="token" type="xsd:string" use="optional" />
                <attribute name="delegationAgreementId" type="xsd:string" use="optional" />
            </extension>
        </complexContent>
    </complexType>

    <complexType name="MultiFactorDomain">
        <sequence/>
        <attribute name="domainMultiFactorEnforcementLevel" type="RAX-AUTH:DomainMultiFactorEnforcementLevelEnum" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Whether or not multifactor authentication should be enforced on this domain
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <simpleType name="StringList">
        <list itemType="xsd:string"/>
    </simpleType>

    <xsd:simpleType name="PolicyAlgorithm">
        <xsd:annotation>
            <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                <p>
                    The list of possible policy algorithms.
                </p>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="if-true-allow"/>
            <xsd:enumeration value="if-false-deny"/>
        </xsd:restriction>
    </xsd:simpleType>

    <simpleType  name="Action">
        <restriction base="xsd:string">
            <enumeration value="GET"/>
            <enumeration value="POST"/>
            <enumeration value="PUT"/>
            <enumeration value="DELETE"/>
            <enumeration value="PATCH"/>
            <enumeration value="*" />
        </restriction>
    </simpleType>

    <simpleType  name="PolicyType">
        <restriction base="xsd:string">
            <enumeration value="json-policy-format"/>
            <enumeration value="standard-policy-format"/>
        </restriction>
    </simpleType>

    <simpleType name="FactorTypeEnum">
        <restriction base="xsd:string">
            <enumeration value="SMS"/>
            <enumeration value="OTP"/>
        </restriction>
    </simpleType>
    <simpleType name="AssignmentTypeEnum">
        <restriction base="xsd:string">
            <enumeration value="DOMAIN"/>
            <enumeration value="TENANT"/>
            <enumeration value="RCN"/>
        </restriction>
    </simpleType>

    <simpleType name="SourceTypeEnum">
        <restriction base="xsd:string">
            <enumeration value="DA"/>
            <enumeration value="USER"/>
            <enumeration value="USERGROUP"/>
            <enumeration value="SYSTEM"/>
        </restriction>
    </simpleType>

    <simpleType name="PrincipalType">
        <restriction base="xsd:string">
            <enumeration value="USER_GROUP"/>
            <enumeration value="USER"/>
        </restriction>
    </simpleType>

    <simpleType name="DelegateType">
        <restriction base="xsd:string">
            <enumeration value="USER_GROUP"/>
            <enumeration value="USER"/>
