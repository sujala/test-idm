    <complexType name="TenantAssignment">
        <complexContent>
            <extension base="RAX-AUTH:RoleAssignment">
                <sequence>
                    <element name="sources" type="RAX-AUTH:AssignmentSources" minOccurs="0" maxOccurs="1"/>
                </sequence>
                <attribute name="forTenants" type="RAX-AUTH:StringList" use="optional">
                    <annotation>
                        <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                            <p>
                                A delimited list of tenant Ids on which the role is assigned. A '*' represents
                                all tenants within the domain. If '*' is one item in a list of tenants, * is simply used.
                            </p>
                        </xsd:documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="AssignmentSources">
        <sequence>
            <element name="source" type="RAX-AUTH:AssignmentSource" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
        <anyAttribute namespace="##other" processContents="lax"/>
