        <attribute name="onRole" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Id of the role to assign.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="onRoleName" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Name of the role. Ignored on assignment. Returned in response to provide additional information
                        on the assigned role.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <anyAttribute namespace="##other" processContents="lax"/>
