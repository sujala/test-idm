    <complexType name="MultiFactorDevices">
        <sequence>
            <element ref="RAX-AUTH:mobilePhones" minOccurs="0" maxOccurs="1"/>
            <element ref="RAX-AUTH:otpDevices" minOccurs="0" maxOccurs="1"/>
