    </complexType>

    <complexType name="ApprovedDomainIds">
        <sequence>
            <element name="approvedDomainId" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="EmailDomains">
        <sequence>
            <element name="emailDomain" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="IdentityProvider">
        <sequence>
            <element ref="RAX-AUTH:publicCertificates" minOccurs="0" maxOccurs="1"/>
            <element ref="RAX-AUTH:approvedDomainIds" minOccurs="0" maxOccurs="1" />
            <element ref="RAX-AUTH:emailDomains" minOccurs="0" maxOccurs="1" />
        </sequence>
        <attribute name="id" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The id of the provider. This is generated by identity.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="name" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The name of the provider. Must consist of only alphanumeric, '-', '.', and be less than 255
                        characters.
                    </p>
                </xsd:documentation>
            </annotation>
            <simpleType>
                <restriction base="xsd:string">
                    <maxLength value="255"/>
                    <pattern value="(\p{L}|\p{Nd}|-|\.)+"/>
                </restriction>
            </simpleType>
        </attribute>
        <attribute name="description" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A description of the provider. Must be less than 255 characters.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="issuer" type="xsd:anyURI" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A unique value the identity provider will use to identify itself within SAML. This value is
                        what will be included as the "issuer". Must be less than 255 characters.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="authenticationUrl" type="xsd:anyURI" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The URL at which users will authenticate with the identity provider. For example, this is to where
                        service providers should redirect to login.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="federationType" type="RAX-AUTH:IdentityProviderFederationTypeEnum" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The federation type of identity provider. External Federated providers will always DOMAIN.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="approvedDomainGroup" type="xsd:string" use="optional" >
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        An approved group of domains for which the identity provider is authorized to authenticate against.
                        The determination of what domains are included in a given group is left for the implementation,
                        as well as what "groups" are available. This attribute is only applicable when federationType
                        is DOMAIN. If this attribute is provided and contains a value, the approvedDomains attribute is ignored.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="metadata" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        The metadata of the provider.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="enabled" type="xsd:boolean" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Whether the Identity Provider is enabled or disabled.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="IdentityProviders">
