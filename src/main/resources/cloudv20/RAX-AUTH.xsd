    <complexType name="AssignmentSource">
        <attribute name="sourceType" type="RAX-AUTH:SourceTypeEnum" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A classification of how the user received the role.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="sourceId" type="xsd:string" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Identifies the specific source of the assignment within the sourceType.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="assignmentType" type="RAX-AUTH:AssignmentTypeEnum" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        Specifies how the role was assigned to the source
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
        <attribute name="forTenants" type="RAX-AUTH:StringList" use="optional">
            <annotation>
                <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                        A list of tenant Ids on which the role is assigned due to the
                        assignment represented by this source.
                    </p>
                </xsd:documentation>
            </annotation>
        </attribute>
    </complexType>

    <complexType name="TenantAssignments">
        <sequence>
            <element ref="RAX-AUTH:tenantAssignment" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
        <anyAttribute namespace="##other" processContents="lax"/>
    </complexType>

    <complexType name="RoleAssignments">
        <sequence>
            <element ref="RAX-AUTH:tenantAssignments" minOccurs="0" maxOccurs="1"/>
        </sequence>
        <anyAttribute namespace="##other" processContents="lax"/>
    </complexType>
</schema>
