    <element name="tokenRevocationRecordDeletionResponse" type="RAX-AUTH:TokenRevocationRecordDeletionResponse" />

    <complexType name="TokenRevocationRecordDeletionResponse">
        <attribute name="id" type="xsd:string" use="required"/>
        <attribute name="deleted" type="xsd:int" use="required"/>
        <attribute name="errors" type="xsd:int" use="required"/>
        <anyAttribute namespace="##other" processContents="lax"/>
    </complexType>

    <complexType name="OTPDevice">
        <complexContent>
            <extension base="RAX-AUTH:MultiFactorDevice">
                <attribute name="verified" type="xsd:boolean">
                    <annotation>
                        <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                            <p>
                                Whether the device has been verified for the associated user.
                            </p>
                        </xsd:documentation>
                    </annotation>
                </attribute>
                <attribute name="name" type="xsd:string" use="optional">
                    <annotation>
                        <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                            <p>
                                Name for the OTP device.
                            </p>
                        </xsd:documentation>
                    </annotation>
                </attribute>
                <attribute name="keyUri" type="xsd:anyURI" use="optional">
                    <annotation>
                        <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                            <p>
                                Key URI for the OTP device (compatible with Google Authenticator KeyUriFormat
                                - https://code.google.com/p/google-authenticator/wiki/KeyUriFormat).
                            </p>
                        </xsd:documentation>
                    </annotation>
                </attribute>
                <attribute name="qrcode" type="xsd:anyURI" use="optional">
                    <annotation>
                        <xsd:documentation xml:lang="EN" xmlns="http://www.w3.org/1999/xhtml">
                            <p>
                                QR Code for key URI for the OTP device.
                            </p>
                        </xsd:documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="OTPDevices">
        <sequence>
            <element ref="RAX-AUTH:otpDevice" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
        <anyAttribute namespace="##other" processContents="lax"/>
