        <entry key="http://docs.openstack.org/identity/api/ext/OS-KSEC2/v1.0">
            <value>os-ksec2</value>
        </entry>
        <entry key="http://docs.rackspace.com/identity/api/ext/RAX-KSKEY/v1.0">
            <value>rax-kskey</value>
        </entry>
    </util:map>

</beans>
