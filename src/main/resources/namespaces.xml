        <entry key="http://docs.rackspace.com/identity/api/ext/RAX-AUTH/v1.0">
            <value>rax-auth</value>
        </entry>

    </util:map>

    <util:map id="osksadmNsPrefixMap"

              key-type="java.lang.String"

