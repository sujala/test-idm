<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
			">

    <context:component-scan use-default-filters="false" base-package="com.rackspace.idm.api.resource.cloud.v20">
        <context:include-filter type="regex" expression=".*DefaultMultiFactorCloud20ServiceSendVerificationCodeIntegrationTest.*" />
    </context:component-scan>
</beans>