{
  "default": {
    "mapping": {
      "rules": [
        {
          "remote": [
            {
              "path":"\/saml2p:Response\/saml2:Assertion\/saml2:Subject\/saml2:NameID"
            },
            {
              "name":"email"
            },
            {
              "path":"\/saml2p:Response\/saml2:Assertion\/saml2:Subject\/saml2:SubjectConfirmation\/saml2:SubjectConfirmationData\/@NotOnOrAfter"
            },
            {
              "name":"domain"
            },
            {
              "name":"roles",
              "multiValue":true
            }
          ],
          "local": {
            "user": {
              "domain":"{3}",
              "name":"{0}",
              "email":"{1}",
              "roles":"{4}",
              "expire":"{2}"
            }
          }
        }
      ],
      "version":"RAX-1"
    }
  },
  "blacklist": {
    "mapping": {
      "rules": [
        {
          "remote": [
            {
              "name": "roles",
              "multiValue": true,
              "blacklist": [
                "lbaas:admin",
                "nova:admin"
              ]
            }
          ],
          "local": {
            "user": {
              "domain": "{D}",
              "name": "{D}",
              "email": "{D}",
              "roles": "{0}",
              "expire": "{D}"
            }
          }
        }
      ],
      "version": "RAX-1",
      "description": " \n  This is an example of a blacklist...note that because we didn't mention\n  user, domain â€” then default values will be used for those.\n"
    }
  },
    "empty": {
      "mapping": {
        "rules": [
          {
            "local": {
              "user": {
                "domain":"{D}",
                "name":"{D}",
                "email":"{D}",
                "roles":"{D}",
                "expire":"{D}"
              }
            }
          }
        ],
        "version":"RAX-1"
      }
    },
  "map_and_roles": {
    "mapping": {
      "rules": [
        {
          "remote": [
            {
              "path":"\/saml2p:Response\/saml2:Assertion\/saml2:Subject\/saml2:NameID"
            },
            {
              "name":"email"
            },
            {
              "path":"\/saml2p:Response\/saml2:Assertion\/saml2:Subject\/saml2:SubjectConfirmation\/saml2:SubjectConfirmationData\/@NotOnOrAfter"
            },
            {
              "name":"domain"
            },
            {
              "path":"for $role in mapping:get-attributes('roles') return for $prod in ('nova', 'lbaas') return concat($prod,':',lower-case($role))",
              "multiValue":true
            }
          ],
          "local": {
            "user": {
              "domain":"{3}",
              "name":"{0}",
              "email":"{1}",
              "roles":"{4}",
              "expire":"{2}"
            }
          }
        }
      ],
      "version":"RAX-1"
    }
  },
  "quoted_attribs": {
    "mapping": {
      "rules": [
        {
          "local": {
            "user": {
              "domain":"{At(\"do\"ma'in')}",
              "name":"{D}",
              "email":"{At('e'mail)}",
              "roles":"{D}",
              "expire":"{D}"
            }
          }
        }
      ],
      "version":"RAX-1"
    }
  },
  "roles_with_spaces": {
  "mapping": {
    "rules": [
      {
        "local": {
          "user": {
            "ext": {
              "value": ["{0}",
                "{2}"],
              "multiValue":true
            },
            "domain":"{D}",
            "name":"{D}",
            "email":"{D}",
            "roles": ["{D}", "{1}"],
            "expire":"{D}"
          },
          "extGroup": {
            "ext2": {
              "value": [
                "{0}", "{2}"
              ],
              "multiValue":true
            }
          }
        },
        "remote" : [
          {
            "name" : "ext2",
            "multiValue" : true
          },
          {
            "name" : "roles2",
            "multiValue" : true
          },
          {
            "name" : "ext3",
            "multiValue" : true
          }
        ]
      }
    ],
    "version":"RAX-1"
  }
}
}
