        "error_message": "tenantType: can only contain alphanumeric characters"
    },
    "with_tenant_type_with_int": {
        "tenant_type": "mosso1234",
        "expected_response": 201
    },
    "with_tenant_type_with_int_uppercase_string": {
        "tenant_type": "MOSSO1234",
        "expected_response": 201
    },
    "with_int_only_tenant_type": {
        "tenant_type": "12345678",
        "expected_response": 201
    },
    "with_tenant_type_with_all_uppercase_string": {
        "tenant_type": "UPPERCASE",
        "expected_response": 201
    },
    "with_tenant_type_with_mixed_case_string": {
        "tenant_type": "UPPERlowerCase",
        "expected_response": 201
    },
    "with_tenant_type_greater_than_15_in_length": {
        "tenant_type": "1234567890abcdefdghi",
